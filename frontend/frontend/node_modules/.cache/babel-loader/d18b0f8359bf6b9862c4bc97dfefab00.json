AIA;AANA;AArBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <ol class=\"post-list\">\r\n      <li class=\"post\" v-for=\"post in publishedPosts\" :key=\"post.title\">\r\n          <span class=\"post__title\">\r\n            <router-link\r\n              :to=\"`/post/${post.slug}`\"\r\n            >{{ post.title }}: {{ post.subtitle }}</router-link>\r\n          </span>\r\n          <span v-if=\"showAuthor\">\r\n            by <AuthorLink :author=\"post.author\" />\r\n          </span>\r\n          <div class=\"post__date\">{{ displayableDate(post.publishDate) }}</div>\r\n        <p class=\"post__description\">{{ post.metaDescription }}</p>\r\n        <ul>\r\n          <li class=\"post__tags\" v-for=\"tag in post.tags\" :key=\"tag.name\">\r\n            <router-link :to=\"`/tag/${tag.name}`\">#{{ tag.name }}</router-link>\r\n          </li>\r\n        </ul>\r\n      </li>\r\n    </ol>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AuthorLink from '@/components/AuthorLink'\r\n\r\nexport default {\r\n  name: 'PostList',\r\n  components: {\r\n    AuthorLink,\r\n  },\r\n  props: {\r\n    posts: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n    showAuthor: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: true,\r\n    },\r\n  },\r\n  computed: {\r\n    publishedPosts () {\r\n      return this.posts.filter(post => post.published)\r\n    }\r\n  },\r\n  methods: {\r\n    displayableDate (date) {\r\n      return new Intl.DateTimeFormat(\r\n        'en-US',\r\n        { dateStyle: 'full' },\r\n      ).format(new Date(date))\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n.post-list {\r\n  list-style: none;\r\n}\r\n\r\n.post {\r\n  border-bottom: 1px solid #ccc;\r\n  padding-bottom: 1rem;\r\n}\r\n\r\n.post__title {\r\n  font-size: 1.25rem;\r\n}\r\n\r\n.post__description {\r\n  color: #777;\r\n  font-style: italic;\r\n}\r\n\r\n.post__tags {\r\n  list-style: none;\r\n  font-weight: bold;\r\n  font-size: 0.8125rem;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}