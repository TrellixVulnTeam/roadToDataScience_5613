AA,SAAA,EAAA;AAAA,OAFA,EAGA,MAHA,CAGA,IAAA,IAAA,CAAA,IAAA,CAHA,CAAA;AAIA;AANA;AAtCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"post\" v-if=\"post\">\r\n      <h2>{{ post.title }}: {{ post.subtitle }}</h2>\r\n      By <AuthorLink :author=\"post.author\" />\r\n      <div>{{ displayableDate(post.publishDate) }}</div>\r\n    <p class=\"post__description\">{{ post.metaDescription }}</p>\r\n    <article>\r\n      {{ post.body }}\r\n    </article>\r\n    <ul>\r\n      <li class=\"post__tags\" v-for=\"tag in post.tags\" :key=\"tag.name\">\r\n        <router-link :to=\"`/tag/${tag.name}`\">#{{ tag.name }}</router-link>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AuthorLink from '@/components/AuthorLink'\r\n\r\nexport default {\r\n  name: 'Post',\r\n  components: {\r\n    AuthorLink,\r\n  },\r\n  data () {\r\n    return {\r\n      post: null,\r\n    }\r\n  },\r\n  async created (){\r\n    const post = await this.$apollo.query({\r\n      query: gql`query ($slug: String!) {\r\n          postBySlug(slug: $slug) {\r\n            title\r\n            subtitle\r\n            publishDate\r\n            metaDescription\r\n            slug\r\n            body\r\n            author {\r\n              user {\r\n                username\r\n                firstName\r\n                lastName\r\n              }\r\n            }\r\n            tags {\r\n              name\r\n            }\r\n          }\r\n        }`,\r\n        variables: {\r\n          slug: this.$route.params.slug,\r\n        },\r\n    })\r\n    this.post = post.data.postBySlug\r\n  },\r\n  methods: {\r\n    displayableDate (date) {\r\n      return new Intl.DateTimeFormat(\r\n        'en-US',\r\n        { dateStyle: 'full' },\r\n      ).format(new Date(date))\r\n    }\r\n  },\r\n}\r\n</script>"],"sourceRoot":"src/components"}]}