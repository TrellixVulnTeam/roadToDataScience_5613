n    popularPosts () {\r\n      return this.posts.filter(post => post.popular)\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nh1 {\r\n    margin-bottom: 30px;\r\n    font-size: 5vh;\r\n    font-weight: lighter;\r\n    color:black;\r\n}\r\n\r\n.card-flyer .ftr div {\r\n    display: inline-block;\r\n    margin-right: 50px;\r\n}\r\n\r\n.tag{\r\n  position: absolute;\r\n  float: left;\r\n  padding-top: 5px;\r\n  min-width: 7vh;\r\n  text-align: left;\r\n  border-style: solid;\r\n  border-width: 1px;\r\n  border-color: rgb(75, 75, 75);\r\n  background-color: rgb(192, 192, 192);\r\n  border-radius: 10px;\r\n  padding:5px;\r\n  text-align: center;\r\n  font-size: 1.5vh;\r\n  color: black;\r\n  font-weight: 600;\r\n  z-index: 100;\r\n}\r\n.avatar {\r\n    width: 36px;\r\n    height: 36px;\r\n    overflow: hidden;\r\n    border-radius: 50%;\r\n    margin-right: 5px;\r\n}\r\n.card-flyer .ftr .stats {\r\n    top: 1px;\r\n    font-size: 14px;\r\n}\r\n.card-flyer .ftr .stats {\r\n    float: right;\r\n    line-height: 30px;\r\n}\r\n.card-flyer{\r\n    min-height: 400px;\r\n}\r\n\r\n#cards_landscape_wrap-2{\r\n  text-align: center;\r\n  background: inherit;\r\n}\r\n#cards_landscape_wrap-2 .container{\r\n  padding-top: 80px; \r\n  padding-bottom: 100px;\r\n}\r\n#cards_landscape_wrap-2 a{\r\n  text-decoration: none;\r\n  outline: none;\r\n}\r\n#cards_landscape_wrap-2 .card-flyer {\r\n  border-radius: 5px;\r\n}\r\n#cards_landscape_wrap-2 .card-flyer .image-box{\r\n  background: #ffffff;\r\n  overflow: hidden;\r\n  box-shadow: 0px 2px 15px rgba(0, 0, 0, 0.50);\r\n  border-radius: 5px;\r\n}\r\n#cards_landscape_wrap-2 .card-flyer .image-box img{\r\n  -webkit-transition:all .9s ease; \r\n  -moz-transition:all .9s ease; \r\n  -o-transition:all .9s ease;\r\n  -ms-transition:all .9s ease; \r\n  width: 100%;\r\n  height: 200px;\r\n}\r\n#cards_landscape_wrap-2 .card-flyer:hover .image-box img{\r\n  opacity: 0.7;\r\n  -webkit-transform:scale(1.15);\r\n  -moz-transform:scale(1.1